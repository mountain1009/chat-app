{
  "name": "chat-app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ssl": "node ./server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint ./ --ext .js,jsx,.ts,.tsx --fix",
    "prisma:studio": "prisma studio --schema core/infra/prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema core/infra/prisma/schema.prisma",
    "prisma:seed": "prisma db seed --schema core/infra/prisma/schema.prisma",
    "prisma:db:reset": "prisma migrate reset --schema core/infra/prisma/schema.prisma",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^3.8.1",
    "@types/styled-components": "^5.1.21",
    "babel-plugin-styled-components": "^2.0.2",
    "next": "12.0.8",
    "next-auth": "^4.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ress": "^4.0.0",
    "styled-components": "^5.3.3",
    "three": "^0.136.0",
    "vanta": "^0.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.14",
    "@types/next-auth": "^3.15.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "prettier": "^2.5.1",
    "prisma": "^3.8.1",
    "storybook-addon-next": "^1.3.1",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
  }
}
