{
  "name": "chat-app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ssl": "node ./server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint ./ --ext .js,jsx,.ts,.tsx --fix",
    "prisma:studio": "prisma studio --schema core/infra/prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema core/infra/prisma/schema.prisma",
    "prisma:seed": "prisma db seed --schema core/infra/prisma/schema.prisma",
    "prisma:generate": "rm -rf core/infra/graphql/generated && prisma generate --schema core/infra/prisma/schema.prisma && prettier --write core/infra/graphql/generated/**/*.ts",
    "prisma:db:reset": "prisma migrate reset --schema core/infra/prisma/schema.prisma",
    "prisma:schema:format": "prisma format --schema core/infra/prisma/schema.prisma",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "dev:all": "yarn dev & yarn prisma:studio"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} core/infra/prisma/seed.ts"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@headlessui/react": "^1.4.3",
    "@next-auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^3.8.1",
    "apollo-server-micro": "^3.6.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.14.1",
    "micro": "^9.3.4",
    "next": "12.0.8",
    "next-auth": "^4.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "ress": "^4.0.0",
    "three": "^0.136.0",
    "type-graphql": "^1.1.1",
    "vanta": "^0.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.14",
    "@types/graphql-fields": "^1.3.4",
    "@types/next-auth": "^3.15.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-tailwindcss": "^3.3.6",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "prisma": "^3.8.1",
    "storybook-addon-next": "^1.3.1",
    "tailwindcss": "^3.0.18",
    "ts-node": "^10.4.0",
    "typegraphql-prisma": "^0.18.4",
    "typescript": "4.5.5"
  }
}
